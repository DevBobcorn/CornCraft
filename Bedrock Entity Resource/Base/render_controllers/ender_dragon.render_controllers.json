      "is_hurt_color": {
        "r": "this",
        "g": "this",
        "b": "this",
        "a": "query.death_ticks > 1.0 ? variable.death_alpha : this"
      }
    },
    "controller.render.ender_dragon_death_main_pass": {
      "geometry": "Geometry.default",
      "materials": [
        { "*": "Material.dissolve_1" }
      ],
      "textures": [ "Texture.default" ]
    }
  }
