{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.donkey.v3": {
      "part_visibility": [
        { "Saddle": "query.is_saddled" },
        { "Bit*": "query.is_saddled" },
        { "Bridle": "query.is_saddled" },
        { "Reins*" : "query.is_saddled && query.has_rider" },
        { "Bag*" : "query.is_chested" },
        { "Ear*" : false}
      ],
      "geometry": "Geometry.default",
      "materials": [
        { "*": "Material.default" }
      ],
      "textures": [ "Texture.donkey", "Texture.markings_none", "Texture.armor_none" ]
    }
  }
}
