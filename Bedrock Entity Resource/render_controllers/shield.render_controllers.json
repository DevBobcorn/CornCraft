{
  "format_version": "1.10",
  "render_controllers": {
    "controller.render.shield": {
      "geometry": "geometry.default",
      "materials": [ { "*": "variable.is_patterned ? (variable.is_enchanted ? material.pattern_enchanted : material.pattern) : (variable.is_enchanted ? material.enchanted : material.default)" } ],
      "textures": [
        "texture.default",
        "texture.enchanted",
        "texture.pattern"
      ]
    }
  }
}
