      "color": {
        "r": 1.0,
        "g": 1.0,
        "b": 1.0,
        "a": "0.7"
      }
    },
    "controller.render.warden_spots_1": {
      "geometry": "Geometry.default",
      "materials": [ { "*": "Material.bioluminescent_layer" } ],
      "textures": [ "Texture.spots_1" ],
      "ignore_lighting": true,
      "color": {
        "r": 1.0,
        "g": 1.0,
        "b": 1.0,
        "a": "variable.spots_1_alpha"
      }
    },
    "controller.render.warden_spots_2": {
      "geometry": "Geometry.default",
      "materials": [ { "*": "Material.bioluminescent_layer" } ],
