{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.wolf_armor": {
      "arrays": {
        "textures": {
          "Array.cracks": [
            "Texture.cracked_none",
            "Texture.cracked_low",
            "Texture.cracked_medium",
            "Texture.cracked_high"
          ]
        }
      },
      "geometry": "Geometry.default",
      "materials": [ { "*": "Material.default" } ],
      "textures": [
        "variable.is_dyed ? Texture.dyed : Texture.default",
        "Array.cracks[variable.cracks_index]"
      ]
    }
  }
}
