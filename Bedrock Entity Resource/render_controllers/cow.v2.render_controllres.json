{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.cow.v2": {
      "arrays": {
        "textures": {
          "Array.textures": [
            "Texture.default",
            "Texture.warm",
            "Texture.cold"
          ]
        },
        "geometries": {
          "Array.geos": [
            "Geometry.default",
            "Geometry.warm",
            "Geometry.cold"
          ]
        }
      },
      "geometry": "Array.geos[v.index]",
      "materials": [ { "*": "v.is_cold ? Material.cold : Material.default" } ],
      "textures": [ "Array.textures[v.index]" ]
    }
  }
}
