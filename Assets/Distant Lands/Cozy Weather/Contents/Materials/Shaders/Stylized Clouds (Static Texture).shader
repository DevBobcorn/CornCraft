				float TIme18 = mulTime17;
				float2 appendResult32 = (float2(( break30.x * 0.0025 * TIme18 ) , ( TIme18 * 0.0025 * break30.z )));
				float2 texCoord14 = IN.ase_texcoord3.xy * float2( 1,1 ) + appendResult32;
